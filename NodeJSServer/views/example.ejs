for i in range(0000,9999):
Data = {'FORM_NAME' : KEY}
r = requests.post(URL, data=Data, cookies=cookies)
      </code></pre>
      <p>For more examples click below:</p>
      <a href="https://realpython.com/python-requests/" class="button buttonTipp w3-my-button" target="_blank">Klick Here</a><% break;
      default: %><h3>No more examples here.<br></h3> <%

      }%>


    </div>
  </div>

  <!-- Footer -->
  <% include partials/footer %>

<!-- END MAIN -->
</div>


